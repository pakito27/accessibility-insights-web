{
    "name": "accessibility-insights-web",
    "version": "1.0.0",
    "description": "Accessibility Insights for Web",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/accessibility-insights-web"
    },
    "engines": {
        "node": ">=10.15.0",
        "npm": ">=6.4.1"
    },
    "main": "src/electron.js",
    "scripts": {
        "build": "grunt",
        "build:all": "grunt build-all",
        "build:dev": "grunt build-dev",
        "build:prod": "grunt build-prod",
        "copyrightheaders": "grunt copyright-check",
        "copyrightheaders:fix": "grunt copyright-add",
        "electron": "electron .",
        "format": "prettier --config prettier.config.js --write \"**/*\"",
        "format-check": "prettier --config prettier.config.js --check \"**/*\"",
        "precheckin": "npm-run-all --serial format-check tslint copyrightheaders build:all test test:e2e",
        "test": "jest --projects src/tests/unit",
        "test:e2e": "jest --projects src/tests/end-to-end --runInBand",
        "tslint": "tslint -c ./tslint.json -p ./tsconfig.json",
        "tslint:fix": "tslint --fix -c ./tslint.json -p ./tsconfig.json",
        "watch": "npm-run-all --parallel --race --print-label watch:grunt watch:test watch:webpack",
        "watch:build": "npm-run-all --parallel --race --print-label watch:grunt watch:webpack",
        "watch:grunt": "grunt watch",
        "watch:test": "jest --watch --coverage false --colors",
        "watch:webpack": "webpack --watch --config-name dev --colors --progress"
    },
    "devDependencies": {
        "@types/applicationinsights-js": "^1.0.7",
        "@types/chrome": "0.0.72",
        "@types/enzyme": "3.1.0",
        "@types/enzyme-adapter-react-16": "^1.0.3",
        "@types/jest": "^24.0.0",
        "@types/jsdom": "^12.2.0",
        "@types/lodash": "^4.14.123",
        "@types/make-dir": "^1.0.3",
        "@types/puppeteer": "^1.11.1",
        "@types/q": "1.5.0",
        "@types/react": "^16.7.18",
        "@types/react-copy-to-clipboard": "^4.2.6",
        "@types/react-dom": "^16.0.11",
        "@types/react-helmet": "^5.0.8",
        "@types/react-router-dom": "^4.3.1",
        "@types/serve-static": "^1.13.2",
        "@types/uuid": "^3.4.4",
        "case-sensitive-paths-webpack-plugin": "^2.2.0",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "express": "^4.16.4",
        "fake-indexeddb": "^2.0.4",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "grunt": "^1.0.4",
        "grunt-bom-removal": "1.0.1",
        "grunt-cli": "^1.3.1",
        "grunt-contrib-clean": "^1.1.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-exec": "^3.0.0",
        "grunt-sass": "^3.0.2",
        "jest": "^24.7.0",
        "jest-circus": "^24.7.0",
        "jest-junit": "^6.3.0",
        "license-check-and-add": "2.3.6",
        "make-dir": "^1.3.0",
        "node-sass": "^4.9.3",
        "npm-run-all": "^4.1.5",
        "preprocess": "^3.1.0",
        "prettier": "^1.16.1",
        "puppeteer": "^1.11.0",
        "script-loader": "0.7.2",
        "serve-static": "^1.13.2",
        "source-map-loader": "^0.2.4",
        "ts-jest": "^24.0.1",
        "ts-loader": "^4.4.2",
        "tslint": "^5.15.0",
        "tslint-microsoft-contrib": "6.1.0",
        "typemoq": "^2.1.0",
        "typescript": "^2.9.2",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1"
    },
    "dependencies": {
        "applicationinsights-js": "^1.0.20",
        "axe-core": "3.2.2",
        "classnames": "^2.2.6",
        "electron": "^5.0.0",
        "electron-renderer": "^2.0.4",
        "lodash": "^4.17.11",
        "office-ui-fabric-react": "6.142.0",
        "q": "1.0.1",
        "react": "^16.7.0",
        "react-copy-to-clipboard": "^5.0.1",
        "react-dom": "^16.7.0",
        "react-helmet": "^5.2.0",
        "react-router-dom": "^4.3.1",
        "requirejs": "2.3.5",
        "uuid": "^3.3.2"
    }
}
